<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.jg.dev.batch">
    <select id="selectList" resultType="com.jg.dev.model.User">
        SELECT
          user_no,
          user_id,
          user_type,
          user_name,
          pwd,
          status,
          create_date
        FROM user
    </select>
</mapper>